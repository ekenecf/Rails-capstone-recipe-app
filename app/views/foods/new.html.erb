<h1>Create new food</h1>

<%= form_with model: @food, url: user_foods_path, method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :measurement_unit %>
  <%= f.text_area :measurement_unit  %>
  <%= f.label :price %>
  <%= f.number_field :price  %>
  <%= f.label :quantity %>
  <%= f.number_field :quantity  %>
  <%= f.submit "Save food" %>
<% end %>
